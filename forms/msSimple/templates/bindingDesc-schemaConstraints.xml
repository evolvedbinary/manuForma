   <binding ident="binding"
            elementName="binding"
            elementLabel="*Binding Description Container (2/2)"
            path="/*:binding"
            currentLevel="1"
            popup="1"
            teiElement="&lt;binding/&gt;">
      <desc>Description of the material, dimensions, and preservation
                            of the binding. Specify in the attribute if the binding is contemporary to the manuscript's production or not with the
                            values "true" or "false".</desc>
      <availableAtts elementName="binding">
         <attDef ident="contemporary" attLabel="contemporary">
            <valList xmlns:rng="http://relaxng.org/ns/structure/1.0"
                     xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                     type="closed"
                     mode="change">
               <valItem ident="true" attLabel="true"/>
               <valItem ident="false" attLabel="false"/>
            </valList>
         </attDef>
      </availableAtts>
      <childElements>
         <child>
            <element ident="condition"
                     label="State of the binding"
                     minOccurs=""
                     maxOccurs=""
                     errorMessage=""/>
            <element ident="decoNote"
                     label="Detailed description of binding"
                     minOccurs=""
                     maxOccurs=""
                     errorMessage=""/>
            <element ident="p"
                     label="Free text box"
                     minOccurs=""
                     maxOccurs=""
                     errorMessage=""/>
            <element ident="origPlace"
                     label="Place of origin"
                     minOccurs=""
                     maxOccurs=""
                     errorMessage=""/>
            <element ident="origDate"
                     label="Date of origin"
                     minOccurs=""
                     maxOccurs=""
                     errorMessage=""/>
         </child>
      </childElements>
   </binding>
   <condition ident="condition"
              elementName="condition"
              elementLabel="State of the binding"
              path="/*:binding/*:condition"
              currentLevel="2"
              popup="2"
              teiElement="&lt;condition/&gt;">
      <desc>Describe the preservation, conservation and restoration
                            history; present state of fragment; damages. Choose from the list a generic description that fits best your overall
                            impression.</desc>
      <availableAtts elementName="condition">
         <attDef ident="key" attLabel="Description">
            <gloss xmlns:rng="http://relaxng.org/ns/structure/1.0"
                   xmlns:sch="http://purl.oclc.org/dsdl/schematron">Description</gloss>
            <valList xmlns:rng="http://relaxng.org/ns/structure/1.0"
                     xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                     type="closed"
                     mode="">
               <valItem ident="deficient" attLabel="deficient"/>
               <valItem ident="good" attLabel="good"/>
               <valItem ident="intact" attLabel="intact"/>
               <valItem ident="other" attLabel="other"/>
            </valList>
         </attDef>
      </availableAtts>
      <childElements>
         <textNode type="input" class="condition"/>
         <child/>
      </childElements>
   </condition>
   <decoNote ident="decoNote"
             elementName="decoNote"
             elementLabel="Detailed description of binding"
