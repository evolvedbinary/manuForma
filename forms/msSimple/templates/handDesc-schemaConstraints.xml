         <attDef ident="hands" attLabel="hands">
            <gloss xmlns:teix="http://www.tei-c.org/ns/Examples"
                   xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                   xmlns:rng="http://relaxng.org/ns/structure/1.0"
                   xml:lang="en"
                   versionDate="2007-06-12">hands</gloss>
            <gloss xmlns:teix="http://www.tei-c.org/ns/Examples"
                   xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                   xmlns:rng="http://relaxng.org/ns/structure/1.0"
                   versionDate="2007-06-12"
                   xml:lang="fr">mains</gloss>
         </attDef>
      </availableAtts>
      <childElements>
         <child>
            <element ident="summary"
                     label="Summary of script"
                     minOccurs=""
                     maxOccurs=""
                     errorMessage=""/>
            <element ident="handNote"
                     label="*Description of scribal hand"
                     minOccurs=""
                     maxOccurs=""
                     errorMessage=""/>
         </child>
