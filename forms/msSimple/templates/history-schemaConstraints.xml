<?xml version="1.0" encoding="UTF-8"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0"
     xmlns:sc="http://www.ascc.net/xml/schematron"
     xmlns:ev="http://www.w3.org/2001/xml-events"
     xmlns:xi="http://www.w3.org/2001/XInclude"
     xmlns:tei="http://www.tei-c.org/ns/1.0"
     xmlns:srophe="https://srophe.app"
     xmlns:xs="http://www.w3.org/2001/XMLSchema"
     xmlns:xf="http://www.w3.org/2002/xforms"
     xmlns:local="http://syriaca.org/ns">
   <history ident="history"
            elementName="history"
            elementLabel="History"
            path="/*:history"
            currentLevel="1"
            popup="1"
            teiElement="&lt;history/&gt;">
      <desc>Give an account about the history of the manuscript and its
                            owners. Distinguish between the previous owners ("Provenance"), and how it came into the present collection
                            ("Acquisition").</desc>
      <availableAtts elementName="history"/>
      <childElements>
         <child>
            <element ident="summary"
                     label="history"
                     minOccurs=""
                     maxOccurs=""
                     errorMessage=""/>
            <element ident="provenance"
                     label="Previous owner"
                     minOccurs=""
                     maxOccurs=""
                     errorMessage=""/>
            <element ident="acquisition"
                     label="Acquisition"
                     minOccurs=""
                     maxOccurs=""
                     errorMessage=""/>
         </child>
      </childElements>
   </history>
   <summary ident="summary"
            elementName="summary"
            elementLabel="summary"
            path="/*:history/*:summary"
            currentLevel="2"
            popup="2"
            teiElement="&lt;summary/&gt;">
      <desc>Outline your observations of the main historical facts of
                            this manuscript or manuscript part.</desc>
      <availableAtts elementName="summary"/>
      <childElements>
         <textNode type="textarea"/>
         <child/>
      </childElements>
   </summary>
   <provenance ident="provenance"
               elementName="provenance"
               elementLabel="Previous owner"
               path="/*:history/*:provenance"
               currentLevel="2"
               popup="2"
               teiElement="&lt;provenance/&gt;">
      <desc>Give an account of how the manuscript passed into the hands of
                            later owners using vestiges they left inside the manuscript or some external records. Use the appropriate fields to
                            describe relevant persons, dates, etc.</desc>
      <availableAtts elementName="provenance"/>
      <childElements>
         <child>
            <element ident="date"
                     label="Date"
                     minOccurs=""
                     maxOccurs=""
                     errorMessage=""/>
            <element ident="p"
                     label="Free text box"
                     minOccurs=""
                     maxOccurs=""
                     errorMessage=""/>
            <element ident="quote"
                     label="Transcription"
                     minOccurs=""
                     maxOccurs=""
                     errorMessage=""/>
            <element ident="locus"
                     label="Folios"
                     minOccurs=""
                     maxOccurs=""
                     errorMessage=""/>
            <element ident="persName"
                     label="Person name"
                     minOccurs=""
                     maxOccurs=""
                     errorMessage=""/>
            <element ident="signatures"
                     label="provenance"
                     minOccurs=""
                     maxOccurs=""
                     errorMessage=""/>
            <element ident="stamp"
                     label="Stamp"
                     minOccurs=""
                     maxOccurs=""
                     errorMessage=""/>
            <element ident="placeName"
                     label="Place name"
                     minOccurs=""
                     maxOccurs=""
                     errorMessage=""/>
         </child>
      </childElements>
   </provenance>
   <date ident="date"
         elementName="date"
         elementLabel="Date"
         path="/*:history/*:provenance/*:date"
         currentLevel="3"
         popup="3"
         teiElement="&lt;date/&gt;">
      <desc>Fill in the date in the agreed-upon calendrical system. Use the "when" field for
                            precise values. In case the date can only be approximated, use the "not After "and "not Before" fields together.</desc>
      <availableAtts elementName="date">
         <attDef ident="notAfter" attLabel="Not after">
            <gloss xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                   xmlns:rng="http://relaxng.org/ns/structure/1.0">Not after</gloss>
         </attDef>
         <attDef ident="notBefore" attLabel="Not before">
            <gloss xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                   xmlns:rng="http://relaxng.org/ns/structure/1.0">Not before</gloss>
         </attDef>
         <attDef ident="when" attLabel="When">
            <gloss xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                   xmlns:rng="http://relaxng.org/ns/structure/1.0">When</gloss>
         </attDef>
      </availableAtts>
      <childElements>
         <textNode type="input" class="date"/>
         <child/>
      </childElements>
   </date>
   <p ident="p"
      elementName="p"
      elementLabel="Free text box"
      path="/*:history/*:provenance/*:p"
      currentLevel="3"
      popup="3"
      teiElement="&lt;p/&gt;">
      <desc>Enter prose text to connect the fields dedicated to specific elements and form a
                            coherent descriptive text.</desc>
      <availableAtts elementName="p"/>
      <childElements>
         <textNode type="textarea"/>
         <child/>
      </childElements>
   </p>
   <quote ident="quote"
          elementName="quote"
          elementLabel="Transcription"
          path="/*:history/*:provenance/*:quote"
          currentLevel="3"
          popup="3"
          teiElement="&lt;quote/&gt;">
      <desc>Quote from the manuscript. Use the "Language" field to specify the
                            language.</desc>
      <availableAtts elementName="quote">
         <attDef ident="xml:lang" attLabel="Language">
            <gloss xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                   xmlns:rng="http://relaxng.org/ns/structure/1.0">Language</gloss>
            <valList xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                     xmlns:rng="http://relaxng.org/ns/structure/1.0"
                     type="closed"
                     mode="change">
               <valItem mode="add" ident="ar"/>
               <valItem mode="add" ident="en"/>
               <valItem mode="add" ident="he"/>
               <valItem mode="add" ident="he-Latn"/>
               <valItem mode="add" ident="ar-Latn"/>
               <valItem mode="add" ident="ar-Hebr"/>
            </valList>
         </attDef>
      </availableAtts>
      <childElements>
         <textNode type="input" class="quote"/>
         <child/>
      </childElements>
   </quote>
   <locus ident="locus"
          elementName="locus"
          elementLabel="Folios"
          path="/*:history/*:provenance/*:locus"
          currentLevel="3"
          popup="3"
          teiElement="&lt;locus/&gt;">
      <desc>Specify the location of a text or another feature within the
                            manuscript. The "target" field is used for single folio. The "from" and "to" fields are used together to describe a range
                            of folios.</desc>
      <availableAtts elementName="locus">
         <attDef ident="cert" attLabel="certainty">
            <gloss xmlns:teix="http://www.tei-c.org/ns/Examples"
                   xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                   xmlns:rng="http://relaxng.org/ns/structure/1.0"
                   versionDate="2009-11-02"
                   xml:lang="en">certainty</gloss>
            <gloss xmlns:teix="http://www.tei-c.org/ns/Examples"
                   xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                   xmlns:rng="http://relaxng.org/ns/structure/1.0"
                   versionDate="2007-12-20"
                   xml:lang="ko">확실성</gloss>
            <gloss xmlns:teix="http://www.tei-c.org/ns/Examples"
                   xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                   xmlns:rng="http://relaxng.org/ns/structure/1.0"
                   versionDate="2008-04-06"
                   xml:lang="es">certeza</gloss>
            <gloss xmlns:teix="http://www.tei-c.org/ns/Examples"
                   xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                   xmlns:rng="http://relaxng.org/ns/structure/1.0"
                   versionDate="2008-03-30"
                   xml:lang="fr">certitude</gloss>
            <gloss xmlns:teix="http://www.tei-c.org/ns/Examples"
                   xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                   xmlns:rng="http://relaxng.org/ns/structure/1.0"
                   versionDate="2007-11-06"
                   xml:lang="it">certezza</gloss>
            <gloss xmlns:teix="http://www.tei-c.org/ns/Examples"
                   xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                   xmlns:rng="http://relaxng.org/ns/structure/1.0"
                   versionDate="2019-09-16"
                   xml:lang="ja">確実性</gloss>
            <desc xmlns:teix="http://www.tei-c.org/ns/Examples"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  versionDate="2009-11-02"
                  xml:lang="en">signifies the degree of certainty associated with the intervention or interpretation.</desc>
            <desc xmlns:teix="http://www.tei-c.org/ns/Examples"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  versionDate="2007-12-20"
                  xml:lang="ko">간섭 또는 해석과 연관된 확실성의 정도를 나타낸다.</desc>
            <desc xmlns:teix="http://www.tei-c.org/ns/Examples"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  versionDate="2007-05-02"
                  xml:lang="zh-TW">表示該更動或詮釋的相關正確度。</desc>
            <desc xmlns:teix="http://www.tei-c.org/ns/Examples"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  versionDate="2008-04-05"
                  xml:lang="ja">当該解釈や調整の確信度を示す．</desc>
            <desc xmlns:teix="http://www.tei-c.org/ns/Examples"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  versionDate="2007-06-12"
                  xml:lang="fr">donne le degré de certitude associée à l'intervention
        ou à l'interprétation.</desc>
            <desc xmlns:teix="http://www.tei-c.org/ns/Examples"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  versionDate="2007-05-04"
                  xml:lang="es">significa el grado de certeza asociado con la
        intervención o la interpretación.</desc>
            <desc xmlns:teix="http://www.tei-c.org/ns/Examples"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  versionDate="2007-01-21"
                  xml:lang="it">corrisponde al grado di certezza associato
        all'intervento o interpretazione</desc>
            <desc xmlns:teix="http://www.tei-c.org/ns/Examples"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  xml:lang="ja"
                  versionDate="2019-09-16">介入や解釈に関する確信度を示す。 </desc>
            <datatype xmlns:teix="http://www.tei-c.org/ns/Examples"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0">
               <dataRef key="teidata.probCert"/>
            </datatype>
         </attDef>
         <attDef ident="from" attLabel="From">
            <gloss xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                   xmlns:rng="http://relaxng.org/ns/structure/1.0">From</gloss>
         </attDef>
         <attDef ident="n" attLabel="number">
            <gloss xmlns:teix="http://www.tei-c.org/ns/Examples"
                   xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                   xmlns:rng="http://relaxng.org/ns/structure/1.0"
                   versionDate="2007-07-02"
                   xml:lang="en">number</gloss>
            <gloss xmlns:teix="http://www.tei-c.org/ns/Examples"
                   xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                   xmlns:rng="http://relaxng.org/ns/structure/1.0"
                   versionDate="2007-12-20"
                   xml:lang="ko">수</gloss>
            <gloss xmlns:teix="http://www.tei-c.org/ns/Examples"
                   xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                   xmlns:rng="http://relaxng.org/ns/structure/1.0"
                   versionDate="2008-04-06"
                   xml:lang="es">número</gloss>
            <gloss xmlns:teix="http://www.tei-c.org/ns/Examples"
                   xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                   xmlns:rng="http://relaxng.org/ns/structure/1.0"
                   versionDate="2008-03-30"
                   xml:lang="fr">nombre</gloss>
            <gloss xmlns:teix="http://www.tei-c.org/ns/Examples"
                   xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                   xmlns:rng="http://relaxng.org/ns/structure/1.0"
                   versionDate="2007-11-06"
                   xml:lang="it">numero</gloss>
            <gloss xmlns:teix="http://www.tei-c.org/ns/Examples"
                   xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                   xmlns:rng="http://relaxng.org/ns/structure/1.0"
                   xml:lang="ja"
                   versionDate="2019-06-08">数値</gloss>
            <desc xmlns:teix="http://www.tei-c.org/ns/Examples"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  versionDate="2005-10-10"
                  xml:lang="en">gives a number (or other label) for an element, which is not necessarily unique within
        the document.</desc>
            <desc xmlns:teix="http://www.tei-c.org/ns/Examples"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  versionDate="2007-12-20"
                  xml:lang="ko">요소의 번호(또는 다른 표지)를 제시하며, 문서 내에서 반드시 고유값일 필요는 없다.</desc>
            <desc xmlns:teix="http://www.tei-c.org/ns/Examples"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  versionDate="2007-05-02"
                  xml:lang="zh-TW">將元素標上一個數字 (或其他標號)，該標號在文件中未必是獨特的。</desc>
            <desc xmlns:teix="http://www.tei-c.org/ns/Examples"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  versionDate="2008-04-05"
                  xml:lang="ja">要素に数値やラベルを与える．これは当該文書中でユニークである必要 はない．</desc>
            <desc xmlns:teix="http://www.tei-c.org/ns/Examples"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  versionDate="2009-05-25"
                  xml:lang="fr">donne un nombre (ou une autre étiquette) pour un
        élément, qui n'est pas nécessairement unique dans le document TEI.</desc>
            <desc xmlns:teix="http://www.tei-c.org/ns/Examples"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  versionDate="2007-05-04"
                  xml:lang="es">proporciona un número (u otra etiqueta) a un elemento
        que no es necesariamente único en el documento.</desc>
            <desc xmlns:teix="http://www.tei-c.org/ns/Examples"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  versionDate="2007-01-21"
                  xml:lang="it">assegna un numero (o altra etichetta) a un elemento
        che non è necessariamente unico all'interno del documento</desc>
            <datatype xmlns:teix="http://www.tei-c.org/ns/Examples"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      maxOccurs="1">
               <dataRef key="teidata.text"/>
            </datatype>
            <remarks xmlns:teix="http://www.tei-c.org/ns/Examples"
                     xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                     xmlns:rng="http://relaxng.org/ns/structure/1.0"
                     versionDate="2013-12-06"
                     xml:lang="en">
               <p>The value of this attribute is always understood to be a single token, even if it contains space or other punctuation characters, and need not be composed of numbers only. It is typically used to specify the numbering of chapters, sections,
          list items, etc.; it may also be used in the specification of a standard reference system
          for the text.</p>
            </remarks>
            <remarks xmlns:teix="http://www.tei-c.org/ns/Examples"
                     xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                     xmlns:rng="http://relaxng.org/ns/structure/1.0"
                     xml:lang="fr"
                     versionDate="2007-06-12">
               <p>L'attribut <att>n</att> peut être employé pour indiquer la numérotation de chapitres,
          sections, items de liste, etc. ; il peut également être employé dans les spécifications
          d'un système standard de référence pour le texte. </p>
            </remarks>
            <remarks xmlns:teix="http://www.tei-c.org/ns/Examples"
                     xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                     xmlns:rng="http://relaxng.org/ns/structure/1.0"
                     xml:lang="es"
                     versionDate="2008-04-06">
               <p>El atributo <att>n</att> se puede utilizar para especificar la enumeración de los
          capítulos, de las secciones, de los items de una lista, etc.; puede también ser utilizado
          en la especificación de un sistema de referencia estándar para el texto.</p>
            </remarks>
            <remarks xmlns:teix="http://www.tei-c.org/ns/Examples"
                     xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                     xmlns:rng="http://relaxng.org/ns/structure/1.0"
                     xml:lang="ja"
                     versionDate="2019-06-08">
               <p>この属性の値は，もし空白やその他の区切り符号があったとしても、つねにひとまとまりのトークンとして解釈され、また数字のみで構成される必要はない。この属性は典型的には章や節，リスト等の構成要素の付番に用いられる。テキストの標準的な参照体系の仕様に含めることができる。</p>
            </remarks>
         </attDef>
         <attDef ident="target" attLabel="Target">
            <gloss xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                   xmlns:rng="http://relaxng.org/ns/structure/1.0">Target</gloss>
         </attDef>
         <attDef ident="to" attLabel="To">
            <gloss xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                   xmlns:rng="http://relaxng.org/ns/structure/1.0">To</gloss>
         </attDef>
      </availableAtts>
      <childElements>
         <textNode type="input" class="locus"/>
         <child/>
      </childElements>
   </locus>
   <persName ident="persName"
             elementName="persName"
             elementLabel="Person name"
             path="/*:history/*:provenance/*:persName"
             currentLevel="3"
             popup="3"
             teiElement="&lt;persName/&gt;">
      <lookup xmlns="http://www.w3.org/1999/xhtml"
              api="/exist/apps/majlis/api/search/persName?format=xml&amp;q="
              formURL="form.xq?form=forms/msSimple/lookup/persName.xhtml"
              elementName="persName"
              formName="persName"
              lookupLabel="Look up persons"/>
      <popup xmlns="http://www.w3.org/1999/xhtml"
             formName="personTEI"
             elementName="persName"
             label="New Persons"
             formURL="form.xq?form=forms/personTEI/index.xhtml"/>
      <desc>Look up the person records in the database to identify the person. Try different
                            spellings in different languages! If no suitable record exists, create a new one. Use the "role" field to specify the
                            person's function to the manuscript (e.g., "owner") from the list. (The "reference" field will be filled out automatically
                            when you link a record to this element.)</desc>
      <availableAtts elementName="persName">
         <attDef ident="ref" attLabel="Person identifier">
            <gloss xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                   xmlns:rng="http://relaxng.org/ns/structure/1.0">Person identifier</gloss>
         </attDef>
      </availableAtts>
      <childElements>
         <textNode type="input" class="persName"/>
         <child/>
      </childElements>
   </persName>
   <signatures ident="signatures"
               elementName="signatures"
               elementLabel="signatures"
               path="/*:history/*:provenance/*:signatures"
               currentLevel="3"
               popup="3"
               teiElement="&lt;signatures/&gt;">
      <availableAtts elementName="signatures"/>
      <childElements>
         <child/>
      </childElements>
   </signatures>
   <stamp ident="stamp"
          elementName="stamp"
          elementLabel="Stamp"
          path="/*:history/*:provenance/*:stamp"
          currentLevel="3"
          popup="3"
          teiElement="&lt;stamp/&gt;">
      <desc>Describe the stamps left by libraries or other institutions, usually to mark
                            manuscripts as their property.</desc>
      <availableAtts elementName="stamp"/>
      <childElements>
         <textNode type="input" class="stamp"/>
         <child/>
      </childElements>
   </stamp>
   <placeName ident="placeName"
              elementName="placeName"
              elementLabel="Place name"
              path="/*:history/*:provenance/*:placeName"
              currentLevel="3"
              popup="3"
              teiElement="&lt;placeName/&gt;">
      <lookup xmlns="http://www.w3.org/1999/xhtml"
              api="/exist/apps/majlis/api/search/placeName?format=xml&amp;q="
              formURL="form.xq?form=forms/msSimple/lookup/placeName.xhtml"
              elementName="placeName"
              formName="placeName"
              lookupLabel="Look up places"/>
      <popup xmlns="http://www.w3.org/1999/xhtml"
             formName="placeTEI"
             elementName="placeName"
             label="New Places"
             formURL="form.xq?form=forms/placeTEI/index.xhtml"/>
      <desc>Look up the place records in the database to specify a settlement or town. Try
                            different spellings in different languages! If no suitable record exists, create a new one. (The "reference" field will be
                            filled out automatically when you link a place record to this element.)</desc>
      <availableAtts elementName="placeName">
         <attDef ident="ref" attLabel="Place identifier">
            <gloss xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                   xmlns:rng="http://relaxng.org/ns/structure/1.0">Place identifier</gloss>
         </attDef>
      </availableAtts>
      <childElements>
         <textNode type="input" class="placeName"/>
         <child/>
      </childElements>
   </placeName>
   <acquisition ident="acquisition"
                elementName="acquisition"
                elementLabel="Acquisition"
                path="/*:history/*:acquisition"
                currentLevel="2"
                popup="2"
                teiElement="&lt;acquisition/&gt;">
      <desc>Give an account of how the manuscript came into the
                            collection where it is now held. Use the available fields to tag relevant persons, dates, etc.</desc>
      <availableAtts elementName="acquisition"/>
      <childElements>
         <child>
            <element ident="placeName"
                     label="Place name"
