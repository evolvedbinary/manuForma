               currentLevel="1"
               popup="1"
               teiElement="&lt;layoutDesc/&gt;">
      <desc>This element contains layout descriptions.</desc>
      <availableAtts elementName="layoutDesc"/>
      <childElements>
         <child>
            <element ident="summary"
                     label="Page layout summary"
                     minOccurs=""
                     maxOccurs=""
                     errorMessage=""/>
