               <valItem ident="complete" attLabel="complete"/>
               <valItem ident="incomplete" attLabel="incomplete"/>
               <valItem ident="normalized" attLabel="normalized"/>
               <valItem ident="supplied" attLabel="supplied"/>
               <valItem ident="uniform" attLabel="uniform"/>
