         elementName="name"
         elementLabel="Team member"
         path="/*:titleStmt/*:respStmt/*:name"
         currentLevel="3"
         popup="3"
         teiElement="&lt;name/&gt;">
      <desc>contains a proper noun or noun phrase.</desc>
      <availableAtts elementName="name">
         <attDef ident="ref" attLabel="ref"/>
         <attDef ident="type" attLabel="type">
            <valList xmlns:rng="http://relaxng.org/ns/structure/1.0"
                     xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                     type="semi"
                     mode="replace">
               <valItem ident="person" attLabel="person"/>
               <valItem ident="place" attLabel="place"/>
               <valItem ident="org" attLabel="org"/>
               <valItem ident="unknown" attLabel="unknown"/>
               <valItem ident="other" attLabel="other"/>
            </valList>
         </attDef>
      </availableAtts>
      <childElements>
         <textNode type="input" class="name"/>
         <child/>
      </childElements>
