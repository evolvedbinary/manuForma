    xmlns:sch="http://purl.oclc.org/dsdl/schematron" xml:lang="en">
    <teiHeader>
        <fileDesc>
            <titleStmt>
                <title>Partial Manuscripts Schema, constrained for usage in XForms</title>
                <author xml:id="PL">Maximilian de Molière</author>
